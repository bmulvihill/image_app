<h3> Create a new Photo Album </h3>
<%= form_for @album, :url => albums_path, :html => { :multipart => true } do |f| %>
  <p><%= f.label :name%><br />
  <%= f.text_field :name %>
  <p><%= f.label :description %>: Enter a description of your album, tags can be added using the #tag format<br />
  <%= f.text_field :description %></p>
  <div><%= f.submit "Submit" %></div>
<% end %>